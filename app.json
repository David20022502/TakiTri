{
  "expo": {
    "name": "TakiTri",
    "slug": "TakiTri",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.png",
    "splash": {
      "image": "./assets/images/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "red"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.takitri"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
